<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yrf.mapper.CustomerInfoMapper">

	<select id="selectCustomerInfoListByCode" parameterType="String" resultType="CustomerInfo">
		select * from customer_info where customer_info_beforecode=#{value}
	</select>

</mapper>



